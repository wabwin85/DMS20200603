
<sqlMap namespace="InvGoodsConfigMap" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://ibatis.apache.org/mapping">
	<alias>
		<typeAlias alias="InvGoodsConfig" assembly="DMS.Model.dll" type="DMS.Model.InvGoodsConfig" />
	</alias>
	<resultMaps>
		<resultMap id="InvGoodsConfigResult" class="InvGoodsConfig">
			<result property="Id" column="Id" type="Guid" dbType="Guid"/>
			<result property="SubCompanyName" column="SubCompanyName" type="string" dbType="nvarchar" /> 
			<result property="BrandName" column="BrandName" type="string" dbType="nvarchar" /> 
			<result property="ProductLine" column="ProductLine" type="string" dbType="nvarchar"/>
			<result property="ProductNameCN" column="ProductNameCN" type="string" dbType="nvarchar"/>
			<result property="InvType" column="InvType" type="string" dbType="nvarchar"/>
			<result property="UpdateTime" column="UpdateTime" type="dateTime" dbType="DateTime"/>
			<result property="Modifier" column="Modifier" type="string" dbType="nvarchar"/>
			<result property="Created" column="Created" type="dateTime" dbType="DateTime"/>
		     <result property="QryCFN" column="QryCFN" type="string" dbType="nvarchar"/>
		</resultMap>
	</resultMaps>
	<statements>
		<select id="SelectInvGoodsConfig" parameterClass="string" resultClass="InvGoodsConfig">
			SELECT Id,ProductLine,ProductNameCN,InvType,CONVERT(varchar(100),UpdateTime,20) AS UpdateTime,Modifier,Convert(varchar(100),Created,20) AS Created,QryCFN,row_number() OVER (ORDER BY [ID] ASC) AS [row_number] FROM InvGoodsConfig
			<dynamic prepend="WHERE">
				<isParameterPresent>Id = #value#</isParameterPresent>
			</dynamic>
		</select>
		<select id="SelectByFilterInvGoodsConfig" parameterClass ="InvGoodsConfig" resultClass="InvGoodsConfig">
			SELECT Id,SubCompanyName,BrandName,ProductLine,ProductNameCN,InvType,CONVERT(varchar(100),UpdateTime,20) AS UpdateTime,Modifier,Convert(varchar(100),Created,20) AS Created,QryCFN,row_number() OVER (ORDER BY [ID] ASC) AS [row_number] FROM InvGoodsConfig
			<dynamic prepend="WHERE">
				<isNotNull prepend="AND" property="Id">Id=#Id#</isNotNull>
				<isNotNull prepend="AND" property="SubCompanyName">SubCompanyName='$SubCompanyName$'</isNotNull>
				<isNotNull prepend="AND" property="BrandName">BrandName='$BrandName$'</isNotNull>
				<isNotNull prepend="AND" property="ProductLine">ProductLine='$ProductLine$'</isNotNull>
				<isNotNull prepend="AND" property="ProductNameCN">ProductNameCN='$ProductNameCN$'</isNotNull>
				<isNotNull prepend="AND" property="InvType">InvType='$InvType$'</isNotNull>
			    <isNotNull prepend="AND" property="QryCFN">QryCFN='$QryCFN$'</isNotNull>
			</dynamic>
		</select>
		<delete id="DeleteInvGoodsConfig" parameterClass="string">
			Delete FROM InvGoodsConfig
			<dynamic prepend="WHERE">
				<!--<isNotNull prepend="AND" property="Id">Id = #Id#</isNotNull>-->
			       <isParameterPresent>Id = #value#</isParameterPresent>
			</dynamic>
		</delete>
	</statements>
</sqlMap>