      <result property="DealerName" column="EL_DealerName" type="string" dbType="nvarchar"/>
      <result property="IsActive" column="EL_IsActive" type="int" dbType="int"/>
      <result property="Remark1" column="EL_Remark1" type="string" dbType="nvarchar"/>
      <result property="Remark2" column="EL_Remark2" type="string" dbType="nvarchar"/>
      <result property="Remark3" column="EL_Remark3" type="string" dbType="nvarchar"/>
