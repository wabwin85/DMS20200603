<?xml version="1.0" encoding="utf-8" ?>
<sqlMap namespace="InventoryAdjustDetailMap" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://ibatis.apache.org/mapping">
    <alias>
        <typeAlias alias="InventoryAdjustDetail" assembly="DMS.Model.dll" type="DMS.Model.InventoryAdjustDetail" />
    </alias>
    <resultMaps>
        <resultMap id="InventoryAdjustDetailResult" class="InventoryAdjustDetail">
            <result property="Quantity" column="IAD_Quantity" type="double" dbType="Real"/>
            <result property="Id" column="IAD_ID" type="Guid" dbType="Guid"/>
            <result property="PmaId" column="IAD_PMA_ID" type="Guid" dbType="Guid"/>
            <result property="IahId" column="IAD_IAH_ID" type="Guid" dbType="Guid"/>
            <result property="LineNbr" column="IAD_LineNbr" type="int" dbType="Int"/>
        </resultMap>
    </resultMaps>
    <statements>

        <select id="SelectInventoryAdjustDetail" parameterClass="string" resultClass="InventoryAdjustDetail">
            SELECT IAD_Quantity AS Quantity,IAD_ID AS Id,IAD_PMA_ID AS PmaId,IAD_IAH_ID AS IahId,IAD_LineNbr AS LineNbr
            FROM InventoryAdjustDetail
            <dynamic prepend="WHERE">
                <isParameterPresent>
                    IAD_ID = #value#
                </isParameterPresent>
            </dynamic>
        </select>
        <select id="SelectByFilterInventoryAdjustDetail" parameterClass="InventoryAdjustDetail" resultClass="InventoryAdjustDetail">
            SELECT IAD_Quantity AS Quantity,IAD_ID AS Id,IAD_PMA_ID AS PmaId,IAD_IAH_ID AS IahId,IAD_LineNbr AS LineNbr
            FROM InventoryAdjustDetail
            <dynamic prepend="WHERE">
                <isNotNull prepend="AND" property="Quantity">IAD_Quantity=#Quantity#</isNotNull>
                <isNotNull prepend="AND" property="Id">IAD_ID=#Id#</isNotNull>
                <isNotNull prepend="AND" property="PmaId">IAD_PMA_ID=#PmaId#</isNotNull>
                <isNotNull prepend="AND" property="IahId">IAD_IAH_ID=#IahId#</isNotNull>
                <isNotNull prepend="AND" property="LineNbr">IAD_LineNbr=#LineNbr#</isNotNull>
            </dynamic>
        </select>
        <select id="SelectInventoryAdjustDetailByHashtable" parameterClass="System.Collections.Hashtable" resultClass="InventoryAdjustDetail">
            SELECT IAD_Quantity AS Quantity,IAD_ID AS Id,IAD_PMA_ID AS PmaId,IAD_IAH_ID AS IahId,IAD_LineNbr AS LineNbr
            FROM InventoryAdjustDetail
            <dynamic prepend="WHERE">
                <isNotNull prepend="AND" property="Quantity">IAD_Quantity=#Quantity#</isNotNull>
                <isNotNull prepend="AND" property="Id">IAD_ID=#Id#</isNotNull>
                <isNotNull prepend="AND" property="PmaId">IAD_PMA_ID=#PmaId#</isNotNull>
                <isNotNull prepend="AND" property="IahId">IAD_IAH_ID=#IahId#</isNotNull>
                <isNotNull prepend="AND" property="LineNbr">IAD_LineNbr=#LineNbr#</isNotNull>
            </dynamic>
        </select>
        <insert id="InsertInventoryAdjustDetail" parameterClass="InventoryAdjustDetail">
            INSERT INTO InventoryAdjustDetail
            (IAD_Quantity,IAD_ID,IAD_PMA_ID,IAD_IAH_ID,IAD_LineNbr)
            VALUES(#Quantity#,#Id#,#PmaId#,#IahId#,#LineNbr#)
        </insert>
        <update id="UpdateInventoryAdjustDetail" parameterClass="InventoryAdjustDetail">
            UPDATE InventoryAdjustDetail
            SET IAD_Quantity=#Quantity#,IAD_ID=#Id#,IAD_PMA_ID=#PmaId#,IAD_IAH_ID=#IahId#,IAD_LineNbr=#LineNbr#
            WHERE IAD_ID = #Id#
        </update>
        <delete id="DeleteInventoryAdjustDetail" parameterClass="string">
            DELETE FROM InventoryAdjustDetail
            WHERE IAD_ID = #value#
        </delete>
        <delete id="DeleteInventoryAdjustDetailByAdjustId" parameterClass="string">
            DELETE FROM InventoryAdjustDetail
            WHERE IAD_IAH_ID = #value#
        </delete>
    </statements>
</sqlMap>
