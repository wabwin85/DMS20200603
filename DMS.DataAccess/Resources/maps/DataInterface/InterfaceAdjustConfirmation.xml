<?xml version="1.0" encoding="utf-8" ?>
<sqlMap namespace="InterfaceAdjustConfirmationMap" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://ibatis.apache.org/mapping">
