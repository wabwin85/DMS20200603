<?xml version="1.0" encoding="utf-8" ?>
<sqlMap namespace="EnterpriseAuthListMap" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://ibatis.apache.org/mapping">
  <alias>
    <typeAlias alias="EnterpriseAuthList" assembly="DMS.Signature.dll" type="DMS.Signature.Model.EnterpriseAuthList" />
