    <statements>

        <select id="SelectInterfaceqrCodeMaster" parameterClass="string" resultClass="InterfaceqrCodeMaster">
            SELECT QRC_ID AS Id,QRC_QRCode AS QrCode,QRC_Status AS Status,QRC_CreateDate AS CreateDate,QRC_UserCode AS UserCode,QRC_Channel AS Channel,QRC_LineNbr AS LineNbr,QRC_FileName AS FileName,QRC_ImportDate AS ImportDate,QRC_ClientID AS Clientid,QRC_BatchNbr AS BatchNbr,QRC_ErrorMsg AS ErrorMsg
            FROM InterfaceQRCodeMaster
            <dynamic prepend="WHERE">
                <isParameterPresent>
                    QRC_ID = #value#
                </isParameterPresent>
            </dynamic>
        </select>
        <select id="SelectByFilterInterfaceqrCodeMaster" parameterClass="InterfaceqrCodeMaster" resultClass="InterfaceqrCodeMaster">
            SELECT QRC_ID AS Id,QRC_QRCode AS QrCode,QRC_Status AS Status,QRC_CreateDate AS CreateDate,QRC_UserCode AS UserCode,QRC_Channel AS Channel,QRC_LineNbr AS LineNbr,QRC_FileName AS FileName,QRC_ImportDate AS ImportDate,QRC_ClientID AS Clientid,QRC_BatchNbr AS BatchNbr,QRC_ErrorMsg AS ErrorMsg
            FROM InterfaceQRCodeMaster
            <dynamic prepend="WHERE">
                <isNotNull prepend="AND" property="Id">QRC_ID=#Id#</isNotNull>
                <isNotNull prepend="AND" property="QrCode">QRC_QRCode=#QrCode#</isNotNull>
                <isNotNull prepend="AND" property="Status">QRC_Status=#Status#</isNotNull>
                <isNotNull prepend="AND" property="UserCode">QRC_UserCode=#UserCode#</isNotNull>
                <isNotNull prepend="AND" property="Channel">QRC_Channel=#Channel#</isNotNull>
                <isNotNull prepend="AND" property="LineNbr">QRC_LineNbr=#LineNbr#</isNotNull>
                <isNotNull prepend="AND" property="FileName">QRC_FileName=#FileName#</isNotNull>
                <isNotNull prepend="AND" property="ImportDate">QRC_ImportDate=#ImportDate#</isNotNull>
                <isNotNull prepend="AND" property="Clientid">QRC_ClientID=#Clientid#</isNotNull>
                <isNotNull prepend="AND" property="BatchNbr">QRC_BatchNbr=#BatchNbr#</isNotNull>
                <isNotNull prepend="AND" property="ErrorMsg">QRC_ErrorMsg=#ErrorMsg#</isNotNull>
            </dynamic>
        </select>
        
        <insert id="InsertInterfaceqrCodeMaster" parameterClass="InterfaceqrCodeMaster">
            INSERT INTO InterfaceQRCodeMaster
            (QRC_ID,QRC_QRCode,QRC_Status,QRC_CreateDate,QRC_UserCode,QRC_Channel,QRC_LineNbr,QRC_FileName,QRC_ImportDate,QRC_ClientID,QRC_BatchNbr,QRC_ErrorMsg)
            VALUES(#Id#,#QrCode#,#Status#,#CreateDate:DateTime:1/1/0001 12:00:00 AM#,#UserCode#,#Channel#,#LineNbr#,#FileName#,#ImportDate:DateTime:1/1/0001 12:00:00 AM#,#Clientid#,#BatchNbr#,#ErrorMsg#)
        </insert>
        <update id="UpdateInterfaceqrCodeMaster" parameterClass="InterfaceqrCodeMaster">
            UPDATE InterfaceQRCodeMaster
            SET QRC_ID=#Id#,QRC_QRCode=#QrCode#,QRC_Status=#Status#,QRC_UserCode=#UserCode#,QRC_Channel=#Channel#,QRC_LineNbr=#LineNbr#,QRC_FileName=#FileName#,QRC_ImportDate=#ImportDate#,QRC_ClientID=#Clientid#,QRC_BatchNbr=#BatchNbr#,QRC_ErrorMsg=#ErrorMsg#
            WHERE QRC_ID = #Id#
        </update>
        <delete id="DeleteInterfaceqrCodeMaster" parameterClass="string">
            DELETE FROM InterfaceQRCodeMaster
            WHERE QRC_ID = #value#
        </delete>
        <procedure id="GC_Interface_QRCodeMaster" parameterMap="GC_Interface_QRCodeMaster_ParameterMap">
            dbo.GC_Interface_QRCodeMaster
        </procedure>
        <select id="SelectInterfaceQRCodeMasterByBatchNbrErrorOnly" parameterClass="string" resultClass="InterfaceqrCodeMaster">
            SELECT QRC_ID AS Id,QRC_QRCode AS QrCode,QRC_Status AS Status,QRC_CreateDate AS CreateDate,QRC_UserCode AS UserCode,QRC_Channel AS Channel,QRC_LineNbr AS LineNbr,QRC_FileName AS FileName,QRC_ImportDate AS ImportDate,QRC_ClientID AS Clientid,QRC_BatchNbr AS BatchNbr,QRC_ErrorMsg AS ErrorMsg
            FROM InterfaceQRCodeMaster
            WHERE QRC_BatchNbr = #value# AND QRC_ErrorMsg IS NOT NULL
            ORDER BY QRC_QRCode
        </select>
        
    </statements>
