<statements>
	<select id="SelectDealerOperationLog" parameterClass="string" resultClass="DealerOperationLog">SELECT DOL_ID AS Id,DOL_Dealer_DMA_ID AS DealerDmaId,DOL_Module_ID AS ModuleId,DOL_Operation_Type AS OperationType,DOL_Operation_User_ID AS OperationUserId,DOL_Operation_Date AS OperationDateFROM DealerOperationLog<dynamic prepend="WHERE"><isParameterPresent>DOL_ID = #value#</isParameterPresent></dynamic></select><select id="SelectByFilterDealerOperationLog" parameterClass="DealerOperationLog" resultClass="DealerOperationLog">SELECT DOL_ID AS Id,DOL_Dealer_DMA_ID AS DealerDmaId,DOL_Module_ID AS ModuleId,DOL_Operation_Type AS OperationType,DOL_Operation_User_ID AS OperationUserId,DOL_Operation_Date AS OperationDateFROM DealerOperationLog<dynamic prepend="WHERE"><isNotNull prepend="AND" property="Id">DOL_ID=#Id#</isNotNull><isNotNull prepend="AND" property="DealerDmaId">DOL_Dealer_DMA_ID=#DealerDmaId#</isNotNull><isNotNull prepend="AND" property="ModuleId">DOL_Module_ID=#ModuleId#</isNotNull><isNotNull prepend="AND" property="OperationType">DOL_Operation_Type=#OperationType#</isNotNull><isNotNull prepend="AND" property="OperationUserId">DOL_Operation_User_ID=#OperationUserId#</isNotNull><isNotNull prepend="AND" property="OperationDate">DOL_Operation_Date=#OperationDate#</isNotNull></dynamic></select><insert id="InsertDealerOperationLog" parameterClass="DealerOperationLog">	INSERT INTO DealerOperationLog (DOL_ID,DOL_Dealer_DMA_ID,DOL_Module_ID,DOL_Operation_Type,DOL_Operation_User_ID,DOL_Operation_Date) 	VALUES(#Id#,#DealerDmaId#,#ModuleId#,#OperationType#,#OperationUserId#,#OperationDate:DateTime:1/1/0001 12:00:00 AM#)</insert><update id="UpdateDealerOperationLog" parameterClass="DealerOperationLog">UPDATE DealerOperationLog  SET DOL_ID=#Id#,DOL_Dealer_DMA_ID=#DealerDmaId#,DOL_Module_ID=#ModuleId#,DOL_Operation_Type=#OperationType#,DOL_Operation_User_ID=#OperationUserId#,DOL_Operation_Date=#OperationDate#WHERE DOL_ID = #Id#</update><delete id="DeleteDealerOperationLog" parameterClass="string">DELETE FROM DealerOperationLogWHERE DOL_ID = #value#</delete>
